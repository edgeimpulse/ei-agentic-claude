{
  "name": "ei-agentic-claude",
  "version": "1.0.0",
  "type": "module",
  "main": "src/cli.ts",
  "bin": {
    "cli": "./src/cli.ts"
  },
  "scripts": {
    "cli": "ts-node-esm src/cli.ts",
    "build": "tsc && node scripts/fix-dist-imports.js && node scripts/generate-integrity-dist.js",
    "build:tsc": "tsc",
    "generate-integrity": "node src/postman/edge-impulse/generate-integrity.js",
    "test": "vitest run --testTimeout=30000",
    "test:unittests": "node scripts/test-reporter.js",
    "list-apis": "node scripts/list-apis.js",
    "smoke": "node ./scripts/smoke-test.js",
    "mcp-server": "node dist/mcp-server.js",
    "test-mcp": "node scripts/test-mcp-simple.js",
    "docker:test": "bash ./scripts/docker-test.sh"
    ,"project:test": "bash ./scripts/run-project-tests.sh"
     ,"assess:projects": "bash ./scripts/record-and-assess.sh"
    ,"prompt:test": "bash ./scripts/run-prompt-tests.sh"
    ,"llm:run": "node scripts/llm-runner.js"
    ,"llm:sim": "node scripts/llm-runner-sim.js"
    ,"test:apply-flow": "bash ./scripts/test-apply-flow.sh"
    ,"configure:projects": "node scripts/configure-org-projects.js"
    ,"llm:integration-sim": "node scripts/llm-integration-sim.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
